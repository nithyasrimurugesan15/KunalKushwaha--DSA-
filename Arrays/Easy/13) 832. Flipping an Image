https://leetcode.com/problems/flipping-an-image/description/

class Solution {
    public int[][] flipAndInvertImage(int[][] image) {
        int n=image.length;
        int[][] res=new int[n][n];
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<image[i].length;j++)
            {
                int num=image[i][n-1-j];
                if(num==0)
                {
                    res[i][j]=1;
                }
                else
                {
                    res[i][j]=0;
                }
           }
        }
        return res;
    }
}
