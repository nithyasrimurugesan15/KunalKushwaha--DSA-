https://leetcode.com/problems/maximum-population-year/submissions/1703860949/

class Solution {
    public int maximumPopulation(int[][] logs) {
        int[] years = new int[101];

        for (int i = 0; i < logs.length; i++) {
            int birth = logs[i][0];
            int death = logs[i][1];

            years[birth - 1950] += 1;

            if (death < 2050) {
                years[death - 1950] -= 1;  
            }
        }

        int max = 0, currSum = 0, res = 0;
        for (int i = 0; i < 101; i++) {
            currSum += years[i];
            years[i] = currSum;
            if (currSum > max) {
                max = currSum;
                res = i;
            }
        }

        return res + 1950;
    }
}
