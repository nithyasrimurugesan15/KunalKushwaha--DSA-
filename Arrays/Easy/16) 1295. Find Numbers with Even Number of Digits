https://leetcode.com/problems/find-numbers-with-even-number-of-digits/

class Solution {
    public int findNumbers(int[] nums) 
    {
       int count=0;
       for(int num:nums)
       {
        if(even(num))
        {
            count++;
        }
       }
       return count;
    }
    boolean even(int num)
    {
        int numdigit=digit(num);
        if(numdigit%2==0)
        {
            return true;
        }
        return false;
    }
    int digit(int num)
    {
        int c=0;
        while(num>0)
        {
           c++;
           num/=10;
        }
        return c;
    }
}
